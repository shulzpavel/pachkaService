{
  "id": null,
  "uid": "pachka-overview",
  "title": "Pachka Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "graph",
      "title": "Services up",
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 6 },
      "targets": [
        {
          "expr": "up{job=\"pachka-services\"}",
          "legendFormat": "{{instance}}"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Latency p95 by service",
      "gridPos": { "x": 0, "y": 6, "w": 12, "h": 6 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, service))",
          "legendFormat": "{{service}}"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Requests by status",
      "gridPos": { "x": 12, "y": 6, "w": 12, "h": 6 },
      "targets": [
        {
          "expr": "sum by (status) (rate(http_requests_total[5m]))",
          "legendFormat": "{{status}}"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Forward duration by target/result",
      "gridPos": { "x": 0, "y": 12, "w": 12, "h": 6 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(forward_request_duration_seconds_bucket[5m])) by (le, target, result))",
          "legendFormat": "{{target}} {{result}}"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Queue size (notifier)",
      "gridPos": { "x": 12, "y": 12, "w": 6, "h": 6 },
      "targets": [
        {
          "expr": "notifier_queue_length",
          "legendFormat": "queue"
        }
      ]
    }
  ]
}
